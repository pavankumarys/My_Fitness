<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.myFitness.myfitness">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
        <activity android:name="com.myFitness.myfitness.countdowntimer"></activity>
        <activity
            android:name=".ui.login.LoginActivity"
            android:label="@string/title_activity_login" />
        <activity android:name="com.myFitness.myfitness.bmi" />
        <activity android:name="com.myFitness.myfitness.back" />
        <activity android:name="com.myFitness.myfitness.shoulder" />
        <activity android:name="com.myFitness.myfitness.tricep" />
        <activity android:name="com.myFitness.myfitness.bicep" />
        <activity android:name="com.myFitness.myfitness.chest" />
        <activity android:name="com.myFitness.myfitness.legs" />
        <activity android:name="com.myFitness.myfitness.homeworkout" />
        <activity android:name="com.myFitness.myfitness.abs" />
        <activity android:name="com.myFitness.myfitness.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.myFitness.myfitness.forgetpassword" />
        <activity android:name="com.myFitness.myfitness.homee" />
        <activity android:name="com.myFitness.myfitness.signup" />
        <activity android:name="com.myFitness.myfitness.login" />
    </application>

</manifest>